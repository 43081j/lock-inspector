{
  "name": "@43081j/lockcheck",
  "version": "0.0.4",
  "description": "Determines changes in package-lock.json files",
  "main": "lib/lockcheck.js",
  "bin": "./lib/cli.js",
  "files": [
    "lib",
    "!lib/test"
  ],
  "scripts": {
    "clean": "rimraf ./lib",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "nyc --reporter=html --reporter=text-summary mocha \"lib/test/**/*_test.js\"",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/43081j/lockcheck.git"
  },
  "author": "James Garbutt (https://github.com/43081j)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/43081j/lockcheck/issues"
  },
  "homepage": "https://github.com/43081j/lockcheck#readme",
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.1",
    "@types/semver": "^6.2.0",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^6.2.2",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "commander": "^4.0.1",
    "semver": "^7.1.1"
  }
}
