{
  "name": "@43081j/lockcheck",
  "version": "0.0.8",
  "description": "Determines changes in package-lock.json files",
  "main": "lib/lockcheck.js",
  "bin": "./lib/cli.js",
  "files": [
    "lib",
    "!lib/test"
  ],
  "scripts": {
    "clean": "rimraf ./lib",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "nyc --reporter=html --reporter=text-summary mocha \"lib/test/**/*_test.js\"",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/43081j/lockcheck.git"
  },
  "author": "James Garbutt (https://github.com/43081j)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/43081j/lockcheck/issues"
  },
  "homepage": "https://github.com/43081j/lockcheck#readme",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.3",
    "@types/semver": "^7.3.3",
    "@types/sinon": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "chai": "^4.2.0",
    "eslint": "^7.8.1",
    "eslint-config-google": "^0.14.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.3",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^6.1.0",
    "semver": "^7.3.2"
  }
}
